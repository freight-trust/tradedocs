<xs:schema targetNamespace="http://www.edi.com.au/EnterpriseService/" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.edi.com.au/EnterpriseService/" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:complexType name="AbstractPackage">
		<xs:sequence>
			<xs:element name="PackType">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:maxLength value="3"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="PackageID" type="xs:string" minOccurs="0"/>
			<xs:element name="TransportRef" type="xs:string" minOccurs="0"/>
			<xs:element name="NumberOfPacks" type="xs:unsignedInt"/>
			<xs:element name="Weight" type="DimensionValue" minOccurs="0"/>
			<xs:element name="Length" type="DimensionValue" minOccurs="0"/>
			<xs:element name="Width" type="DimensionValue" minOccurs="0"/>
			<xs:element name="Height" type="DimensionValue" minOccurs="0"/>
			<xs:element name="Volume" type="DimensionValue" minOccurs="0"/>
			<xs:element name="GoodsDescription" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AdditionalCustomsInformation">
		<xs:sequence>
			<xs:element name="CustomsDetailType" type="xs:NMTOKEN"/>
			<xs:element name="CustomsDetailValue" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressBase">
		<xs:sequence>
			<xs:element name="AddressLine1" type="xs:string" minOccurs="0"/>
			<xs:element name="AddressLine2" type="xs:string" minOccurs="0"/>
			<xs:element name="AddressCode" type="xs:string" minOccurs="0"/>
			<xs:element name="CityOrSuburb" type="xs:string" minOccurs="0"/>
			<xs:element name="StateOrProvince" type="xs:string" minOccurs="0"/>
			<xs:element name="PostCode" type="xs:string" minOccurs="0"/>
			<xs:element name="TelephoneNumbers" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TelephoneNumber" type="TelephoneNumber" minOccurs="0" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Email" type="xs:string" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="AddressCapability">
		<xs:attribute name="IsMainAddress" type="TrueFalse" use="optional"/>
		<xs:attribute name="AddressType">
			<xs:simpleType>
				<xs:restriction base="xs:NMTOKEN">
					<xs:enumeration value="MAIN"/>
					<xs:enumeration value="OFC"/>
					<xs:enumeration value="PST"/>
					<xs:enumeration value="ARM"/>
					<xs:enumeration value="APM"/>
					<xs:enumeration value="PIC"/>
					<xs:enumeration value="DLV"/>
					<xs:enumeration value="PAD"/>
					<xs:enumeration value="SQM"/>
					<xs:enumeration value="MSC"/>
					<xs:enumeration value="RSD"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="AddressReference">
		<xs:sequence>
			<xs:element name="AddressSequenceRef" type="xs:int"/>
			<xs:element name="Organisation" type="Organisation"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Charges">
		<xs:sequence>
			<xs:element name="Charge" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ChargeType" type="xs:NMTOKEN"/>
						<xs:element name="ChargeAmount" type="FinancialValue"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Fees">
		<xs:sequence>
			<xs:element name="Fee" maxOccurs="unbounded">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="FeeType" type="xs:NMTOKEN"/>
						<xs:element name="FeeAmount" type="FinancialValue"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContactReference">
		<xs:sequence>
			<xs:element name="ContactSequenceRef" type="xs:int"/>
			<xs:element name="Organisation" type="Organisation"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Container">
		<xs:sequence>
			<xs:element name="ContainerNumber" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Unique identifier for container.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="12"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="ContainerCount" type="xs:int" default="1" minOccurs="0"/>
			<xs:element name="ContainerType" type="ContainerType" minOccurs="0"/>
			<xs:element name="Seal" type="xs:string" minOccurs="0"/>
			<xs:element name="PackingMode" type="ContainerMode"/>
			<xs:element name="DeliveryMode" type="xs:string" minOccurs="0"/>
			<xs:element name="EstimatedDelivery" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="IsShipperOwnedContainer" type="xs:boolean" minOccurs="0"/>
			<xs:element name="LCLAvailable" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="Weight" type="xs:decimal" minOccurs="0"/>
			<xs:element name="FCLAvailable" type="xs:dateTime" minOccurs="0"/>
			<xs:element name="BookingReference" type="xs:string" minOccurs="0"/>
			<xs:element name="IsArrivingAtCTOByRail" type="xs:boolean" minOccurs="0"/>
			<xs:element name="IsEmptyContainer" type="xs:boolean" minOccurs="0"/>
			<xs:element name="IsDamaged" type="xs:boolean" minOccurs="0"/>
			<xs:element name="CommodityCode" type="xs:string" minOccurs="0"/>
			<xs:element name="ImportProcess" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ContainerProcess">
							<xs:sequence>
								<xs:element name="WharfUnload" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="WharfGateOut" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="EstimatedDelivery" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="DeliverEmptyTo" type="AddressReference" minOccurs="0"/>
								<xs:element name="EmptyReady" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="EmptyReturnRequiredBy" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="EmptyReturnedOn" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="DetentionCharge" type="xs:decimal" minOccurs="0"/>
								<xs:element name="DetentionDays" type="xs:string" minOccurs="0"/>
								<xs:element name="StorageCharge" type="xs:decimal" minOccurs="0"/>
								<xs:element name="StorageDays" type="xs:string" minOccurs="0"/>
								<xs:element name="PickupByRail" type="xs:boolean" minOccurs="0"/>
								<xs:element name="HeldForFCLTransitStaging" type="xs:boolean" minOccurs="0"/>
								<xs:element name="FCLAvailable" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="FCLStorage" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="LCLAvailable" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="LCLStorage" type="xs:dateTime" minOccurs="0"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="ExportProcess" minOccurs="0">
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ContainerProcess">
							<xs:sequence>
								<xs:element name="BookingReference" type="xs:string" minOccurs="0"/>
								<xs:element name="EmptyRequiredBy" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="EstimatedFullPickup" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="PickupEmptyFrom" type="AddressReference" minOccurs="0"/>
								<xs:element name="ContainerYardGateOut" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="WharfGateIn" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="ShippedOnboard" type="xs:dateTime" minOccurs="0"/>
								<xs:element name="IsArrivingAtCTOByRail" type="xs:boolean" minOccurs="0"/>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Custom" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="CustomAttribute1" type="xs:string" minOccurs="0"/>
						<xs:element name="Date1" type="xs:dateTime" minOccurs="0"/>
						<xs:element name="Decimal1" type="xs:decimal" minOccurs="0"/>
						<xs:element name="Flag1" type="xs:boolean" minOccurs="0"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ContainerType">
		<xs:sequence>
			<xs:element name="USContainerCode" type="xs:NMTOKEN" minOccurs="0"/>
			<xs:element name="Length" type="xs:decimal" minOccurs="0"/>
			<xs:element name="Width" type="xs:decimal" minOccurs="0"/>
			<xs:element name="Height" type="xs:decimal" minOccurs="0"/>
			<xs:element name="ContainerCode" type="xs:string" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="ISOCode" type="xs:NMTOKEN"/>
	</xs:complexType>
	<xs:complexType name="CustomsEntry">
		<xs:sequence>
			<xs:element name="CustomsEntryNumber" type="CustomsEntryNumber" minOccurs="0"/>
			<xs:element name="EntryDate" type="xs:date" minOccurs="0"/>
			<xs:element name="CustomsFactor" type="xs:decimal" minOccurs="0"/>
			<xs:element name="ExchangeRate" type="xs:decimal" minOccurs="0"/>
			<xs:element name="CustomsValue" type="FinancialValue" minOccurs="0"/>
			<xs:element name="TransportAndInsurance" type="FinancialValue" minOccurs="0"/>
			<xs:element name="Charges" type="Charges" minOccurs="0"/>
			<xs:element name="CustomsEntryLines" type="CustomsEntryLines" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomsEntryLine">
		<xs:sequence>
			<xs:element name="Fees" type="Fees" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomsEntryLines">
		<xs:sequence>
			<xs:element name="CstomsEntryLine" type="CustomsEntryLine" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomsEntries">
		<xs:sequence>
			<xs:element name="CustomsEntry" type="CustomsEntry" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomsEntryNumber">
		<xs:sequence>
			<xs:element name="Country" type="CountryCode"/>
			<xs:element name="Type" type="xs:NMTOKEN" minOccurs="0"/>
			<xs:element name="Number" type="xs:NMTOKEN" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CustomsEntryNumbers">
		<xs:sequence>
			<xs:element name="CustomsEntryNumber" type="CustomsEntryNumber" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="Declaration">
		<xs:annotation>
			<xs:documentation>Declaration specific information</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Importer" type="Organisation" minOccurs="0">
				<xs:annotation>
					<xs:documentation>This is only required if the Importer is different to the consignee on the shipment</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ManifestID" type="xs:string" minOccurs="0"/>
			<xs:element name="AddCustomsDetails" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AddCustomsDetail" type="AdditionalCustomsInformation" maxOccurs="unbounded"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="BillContainerPacks" minOccurs="0">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="BillContainerPack" maxOccurs="unbounded">
							<xs:complexType>
								<xs:sequence>
									<xs:element name="BillNumber" type="xs:string" minOccurs="0"/>
									<xs:element name="MasterbillNumber" type="xs:string" minOccurs="0"/>
									<xs:element name="ContainerNumber" type="xs:string" minOccurs="0"/>
									<xs:element name="PackQty" type="DimensionValue" minOccurs="0"/>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="GoodsOrigin" type="CountryCode" minOccurs="0"/>
			<xs:element name="IsDocuments" type="TrueFalse" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specify whether the goods are documents or non-documents.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IsFumigationCert" type="xs:boolean" minOccurs="0"/>
			<xs:element name="IsPerishableGoods" type="xs:boolean" minOccurs="0"/>
			<xs:element name="IsPersonalEffects" type="xs:boolean" minOccurs="0"/>
			<xs:element name="IsHazardousGoods" type="xs:boolean" minOccurs="0"/>
			<xs:element name="IsTimber" type="xs:boolean" minOccurs="0"/>
			<xs:element name="IsSAC" type="xs:boolean" minOccurs="0"/>
			<xs:element name="IsSurplus" type="TrueFalse" minOccurs="0"/>
			<xs:element name="PaymentTerms" type="xs:NMTOKEN" minOccurs="0"/>
			<xs:element name="BondedWarehouse" type="AddressReference" minOccurs="0"/>
			<xs:element name="LandedCostingHeader" type="LandedCostingHeader" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="DimensionValue">
		<xs:simpleContent>
			<xs:extension base="xs:decimal">
				<xs:attribute name="DimensionType" type="xs:NMTOKEN" use="optional"/>
				<xs:attribute name="Description" type="xs:string" use="optional"/>
			</xs:extension>
		</xs:simpleContent>
	</xs:complexType>
	<xs:complexType name="DocAddress">
